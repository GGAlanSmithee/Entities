{
  "name": "Entities",
  "version": "0.0.1",
  "description": "efficient and easy-to-use Entity-Component System in JavaScript",
  "main": "Gruntfile.js",
  "scripts": {
    "babel": "babel ./dist/es6.js --out-file ./dist/es5.js",
    "bundle": "node ./bundle",
    "clean": "./node_modules/.bin/rimraf ./dist/*",
    "jshint": "jshint ./src/core",
    "test": "./node_modules/.bin/istanbul cover _mocha test/*  -- --compilers js:babel/register -R dot",
    "prebuild": "npm run jshint && npm run clean && npm run bundle",
    "build": "npm run babel",
    "predeploy": "npm run build",
    "deploy": "browserify ./example/client.js -t babelify --outfile ./dist/client.js"
  },
  "directories": {
    "examples": "examples"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GGAlanSmithee/Entities"
  },
  "keywords": [
    "javascript",
    "ces",
    "ecs",
    "entity",
    "component"
  ],
  "author": "Alan Smithee",
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.5.7",
    "babelify": "^6.1.2",
    "browserify": "^10.2.4",
    "chai": "^3.0.0",
    "esperanto": "^0.7.2",
    "istanbul": "^0.3.15",
    "jshint": "^2.8.0",
    "mocha": "^2.2.5",
    "rimraf": "^2.4.0",
    "sinon": "^1.15.3"
  }
}
